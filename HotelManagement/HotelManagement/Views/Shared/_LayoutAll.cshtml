@{

    string activeTab = string.Empty;
    string activeTabName = "something...";
}
<!doctype html>
<html lang="en">
<head>
    <title>LuxuryHotel a Hotel Template</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,700,900|Rubik:300,400,700" rel="stylesheet">
    <link rel="stylesheet" href="~/template/landingpage/css/bootstrap.css" />
    <link rel="stylesheet" href="~/template/landingpage/css/bootstrap.css" />
    <link rel="stylesheet" href="~/template/landingpage/css/animate.css" />
    <link rel="stylesheet" href="~/template/landingpage/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/template/landingpage/fonts/ionicons/css/ionicons.min.css" />
    <link rel="stylesheet" href="~/template/landingpage/fonts/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/template/landingpage/css/magnific-popup.css">

    <!-- Theme Style -->

    <link href="~/template/templates/luxury-hotel-master/luxury-hotel-master/css/style.css" rel="stylesheet" />

</head>
<body>

    <header role="banner">

        <nav class="navbar navbar-dark bg-dark">
            <div class="container">
                <a class="navbar-brand" href="index.html">Relax Stay Inn</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExample05" aria-controls="navbarsExample05" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarsExample05">
                    <ul class="navbar-nav ml-auto pl-lg-5 pl-0">
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Home" asp-action="Index" style="color: purple;">Home</a>
                        </li>
                        <!-- Additional menu items -->
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Home" asp-action="About" style="color: purple;">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Home" asp-action="Contact" style="color: purple;">Contact</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Account" asp-action="Login" style="color: purple;">Admin LogIn</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-action="BookNow">Book Now</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <!-- END header -->
    @RenderBody()


    <footer class="site-footer">
        <div class="container">
            <div class="row mb-5">
                <div class="col-md-4">
                    <h3>Phone Support</h3>
                    <p>24/7 Call us now.</p>
                    <p class="lead"><a href="tel://">+ 1 332 3093 323</a></p>
                </div>
                <div class="col-md-4">
                    <h3>Connect With Us</h3>
                    <p>We are socialized. Follow us</p>
                    <p>
                        <a href="#" class="pl-0 p-3"><span class="fa fa-facebook"></span></a>
                        <a href="#" class="p-3"><span class="fa fa-instagram"></span></a>
                    </p>
                </div>
                <div class="col-md-4">
                    <h3>Connect With Us</h3>
                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Maxime, odio.</p>
                    <form action="#" class="subscribe">
                        <div class="form-group">
                            <button type="submit"><span class="ion-ios-arrow-thin-right"></span></button>
                            <input type="email" class="form-control" placeholder="Enter email">
                        </div>

                    </form>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-md-7 text-center">
                    &copy; <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                </div>
            </div>
        </div>
    </footer>
    <!-- END footer -->
    <!-- loader -->
    <div id="loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#f4b214" /></svg></div>

    <script src="~/template/landingpage/js/jquery-3.2.1.min.js"></script>
    <script src="~/template/landingpage/js/jquery-migrate-3.0.0.js"></script>
    <script src="~/template/landingpage/js/popper.min.js"></script>
    <script src="~/template/landingpage/js/bootstrap.min.js"></script>
    <script src="~/template/landingpage/js/owl.carousel.min.js"></script>
    <script src="~/template/landingpage/js/jquery.waypoints.min.js"></script>
    <script src="~/template/landingpage/js/jquery.stellar.min.js"></script>

    <script src="~/template/landingpage/js/jquery.magnific-popup.min.js"></script>
    <script src="~/template/landingpage/js/magnific-popup-options.js"></script>

    <script src="~/template/landingpage/js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
